page {
  padding-bottom: 80rpx;
  background: #F5F5F5;
}

/* 姓名、号码、地址 */
.person {
  width: 100%;
  height: 190rpx;
  padding: 18rpx 20rpx;
  box-sizing: border-box;
  color: #FFFFFF;
  background-size: 100% 100%;
  background-image: url("data:image/png;base64,");
}

.person > text {
  display: block;
  font-size: 22rpx;
}

.person .adress {
  display: flex;
  align-items: center;
  margin: 16rpx 0 6rpx;
}

.adress text {
  font-size: 40rpx;
  font-weight: bold;
}

.adress image {
  width: 14rpx;
  height: 24rpx;
  margin-left: 20rpx;
}

.name-phone text {
  margin-right: 66rpx;
  font-size: 30rpx;
}

/* 已经下单的top样式 */
.delivery {
  display: flex;
  align-items: center;
  height: 100rpx;
  padding: 0 20rpx;
  background-size: 100% 100%;
  background-image: url("data:image/png;base64,");
}

.delivery image {
  width: 66rpx;
  height: 52rpx;
}

.delivery text {
  margin-left: 20rpx;
  font-size: 40rpx;
  color: #FFFFFF;
}

/* 订单体 */
.order {
  margin: 20rpx 0;
  background: white;
}

.order .store-name {
  display: block;
  line-height: 78rpx;
  padding: 0 20rpx;
  border-bottom: 1px solid #F5F5F5;
  font-weight: bold;
  font-size: 26rpx;
  color: #282828;
}

.product-list {
  padding: 0 20rpx;
}

.product-item {
  display: flex;
  align-items: center;
  height: 128rpx;
  border-bottom: 2rpx solid #F5F5F5;
}

.product-item image {
  width: 88rpx;
  height: 88rpx;
  margin: 20rpx 20rpx 20rpx 0;
  border-radius: 8rpx;
}

.product-item .profile {
  flex: 1;
  display: flex;
  align-items: center;
  height: 100%;
}

.profile .name-specs {
  margin-right: 40rpx;
  max-width: 400rpx;
}

.name-specs text {
  display: block;
  color: #222222;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  word-break: break-all;
}

.name-specs .name {
  margin-bottom: 10rpx;
  font-size: 26rpx;
}

.name-specs .specs {
  font-size: 22rpx;
}

.profile .count {
  font-size: 26rpx;
  color: #222222;
}

.product-item .money {
  display: block;
  max-width: 110rpx;
  margin-left: 20rpx;
  font-size: 30rpx;
  color: #222222;
}

.calc-money {
  padding: 0 20rpx;
  line-height: 80rpx;
  font-size: 26rpx;
  color: #666666;
}

.calc-money text:nth-of-type(2) {
  float: right;
  font-weight: bold;
  color: #F32939;
}

.calc-money text:nth-of-type(3) {
  float: right;
  color: #222222;
}

 /*已经下单的订单信息 */
.order-message {
  margin-top: 20rpx;
  background: white;
}

.order-message .top-title {
  display: block;
  padding-left: 20rpx;
  line-height: 78rpx;
  font-weight: bold;
  font-size: 26rpx;
  color: #282828;
  border-bottom: 1rpx solid #F5F5F5;
}

.order-message .adress-name {
  padding: 22rpx 20rpx;
  border-bottom: 1rpx solid #F5F5F5;
}

.adress-name .adress text:nth-of-type(1) {
  display: block;
  float: left;
  font-size: 26rpx;
  color: #222222;
}

.adress-name .adress text:nth-of-type(2) {
  display: block;
  float: right;
  font-size: 22rpx;
  color: #666666;
}

.adress-name .adress:after {
  content: '';
  display: block;
  clear: both;
}

.adress-name .name {
  display: block;
  text-align: right;
  font-size: 22rpx;
  color: #666666;
}

.others {
  padding: 0 20rpx;
  line-height: 76rpx;
  border-bottom: 1px solid #F5F5F5;
}

.others text:nth-of-type(1) {
  font-size: 26rpx;
  color: #222222;
}

.others text:nth-of-type(2) {
  float: right;
  font-size: 22rpx;
  color: #666666;
}

/* 备注 */
.remarks {
  display: flex;
  padding: 20rpx;
  background: white;
}

.remarks text {
  line-height: 60rpx;
  font-weight: bold;
  font-size: 30rpx;
  color: #222222;
}

.remarks textarea {
  flex: 1;
  height: 174rpx;
  padding: 20rpx;
  box-sizing: border-box;
  background: #F5F5F5;
  border-radius: 8rpx;
  font-size: 26rpx;
  color: #9A9A9A;
}

/* 用户必读 */
.need-read {
  margin-top: 20rpx;
  padding: 20rpx;
  background: white;
}

.need-read .read-title {
  display: block;
  line-height: 66rpx;
  padding-left: 10rpx;
  font-weight: bold;
  font-size: 30rpx;
  color: #222222;
  position: relative;
  z-index: 999;
}

.need-read .read-title:before {
  content: '';
  position: absolute;
  left: -4rpx;
  top: 6rpx;
  display: block;
  width: 38rpx;
  height: 38rpx;
  border-radius: 100%;
  background: #F32939;
  z-index: -1;
}

.read-item {
  margin-bottom: 14rpx;
}

.read-item:after {
  content: '';
  display: block;
  clear: both;
}

.read-item .black-dot {
  float: left;
  margin-top: 16rpx;
  margin-right: 10rpx;
  width: 8rpx;
  height: 8rpx;
  border-radius: 4rpx;
  background: black;
}

.item-content  {
  float: left;
  width: calc(100% - 30rpx);
}

.item-content text {
  display: block;
  line-height: 38rpx;
  font-size: 22rpx;
  color: #444444;
}

/* 提示 */
.tips {
  padding: 20rpx;
  font-size: 22rpx;
  color: #F32939;
}

/* 合计 */
.amount {
  position: fixed;
  bottom: 0;
  padding-left: 20rpx;
  width: 100%;
  line-height: 78rpx;
  font-size: 26rpx;
  background: white;
}

.amount .total {
  color: #222222;
}

.amount .money {
  font-weight: bold;
  color: #F32939;
}

.amount .pay {
  display: block;
  float: right;
  width: 200rpx;
  height: 100%;
  background: #F32939;
  text-align: center;
  font-size: 30rpx;
  color: white;
}

.amount .pay.active {
  background: #222222;
}

.amount .pay-count {
  display: block;
  float: right;
  padding: 0 20rpx;
  color: #666666;
}