page {
  background: #F5F5F5;
}

.input-list {
  background: white;
}

.input-item {
  display: flex;
  align-items: center;
  padding: 0 20rpx;
  height: 90rpx;
  border-bottom: 2rpx solid #F5F5F5;
}

.input-item > text {
  display: block;
  width: 140rpx;
  font-size: 30rpx;
  color: #444444;
}

.input-item .input {
  flex: 9;
  font-size: 30rpx;
  color: #111111;
}

.enter-adress {
  padding-right: 0;
}

.enter-adress .img-wrap {
  display: flex;
  flex-direction: row-reverse;
  align-items: center;
  width: 100rpx;
  height: 100%;
  margin-left: 20rpx;
}

.enter-adress image {
  float: right;
  width: 10rpx;
  height: 20rpx;
  margin-right: 20rpx;
}

.enter-adress .input {
  color: #777777;
}

.enter-adress .input.active {
  color: #111111;
}

.set-default {
  display: flex;
  align-items: center;
  height: 90rpx;
  padding: 0 20rpx;
  margin-top: 20rpx;
  background: white;
  position: relative;
}

.set-default text {
  font-size: 30rpx;
  color: #444444;
}

.switch {
  position: absolute;
  right: 20rpx;
  zoom: .6;
}

.btn-group {
  position: fixed;
  bottom: 50rpx;
  width: 100%;
  padding: 0 69rpx;
  box-sizing: border-box;
}

.btn-group .save {
  height: 90rpx;
  line-height: 90rpx;
  margin-bottom: 30rpx;
  font-size: 30rpx;
  color: white;
  border-radius: 8rpx;
  background-size: 100% 100%;
  background-image: url("data:image/png;base64,");

}

.btn-group .delete {
  height: 90rpx;
  line-height: 90rpx;
  font-size: 30rpx;
  color: #F32939;
  border: 1rpx solid #F32939;
  border-radius: 8rpx;
  box-sizing: border-box;
}

.auth-location {
  position: fixed;
  left: 0;
  top: 0;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  height: 100%;
  padding: 0 50rpx;
  box-sizing: border-box;
  background: rgba(0, 0, 0, .42);
}

.auth-box {
  border-radius: 10rpx;
  background: white;
}

.auth-box text {
  display: block;
  padding: 0 30rpx;
  margin: 46rpx 0;
  line-height: 66rpx;
  text-align: center;
  font-size: 40rpx;
  color: #333333;
}

.auth-box button {
  width: 100%;
  line-height: 100rpx;
  border-radius: 0;
  border-bottom-left-radius: 10rpx;
  border-bottom-right-radius: 10rpx;
  background: #F32939;
  letter-spacing: 6rpx;
  font-size: 42rpx;
  color: white;
}