/* 姓名、号码、地址 */
.person {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 190rpx;
  padding: 18rpx 20rpx;
  box-sizing: border-box;
  color: #FFFFFF;
  z-index: 9999;
  background-size: 100% 100%;
  background-image: url("data:image/png;base64,");
}

.person > text {
  display: block;
  font-size: 22rpx;
}

.person .adress {
  display: flex;
  align-items: center;
  margin: 16rpx 0 6rpx;
}

.adress text {
  display: block;
  max-width: 650rpx;
  font-size: 40rpx;
  font-weight: bold;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.adress image {
  width: 14rpx;
  height: 24rpx;
  margin-left: 20rpx;
}

.name-phone text {
  margin-right: 66rpx;
  font-size: 30rpx;
}

.order-list {
  padding: 0 20rpx 76rpx;
  margin-top: 190rpx;
}

.order-item {
  display: flex;
  align-items: center;
  height: 240rpx;
  border-bottom: 1px solid #F5F5F5;
  position: relative;
}

.right-go {
  position: absolute;
  top: 30rpx;
  right: 40rpx;
  width: 18rpx;
  height: 30rpx;
}

.order-item .title-img {
  width: 200rpx;
  height: 200rpx;
  border-radius: 8rpx;
}

.order-item .order-specs {
  flex: 1;
  padding: 20rpx;
  box-sizing: border-box;
  max-width: 500rpx;
  height: 100%;
  position: relative;
}

.order-specs text {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.order-specs text:nth-of-type(1) {
  display: block;
  line-height: 60rpx;
  font-size: 30rpx;
  color: #222222;
}

.order-specs text:nth-of-type(2) {
  display: block;
  font-size: 22rpx;
  color: #666666;
}

.order-specs text:nth-of-type(3) {
  display: block;
  position: absolute;
  bottom: 30rpx;
  font-size: 30rpx;
  color: #C41D17;
}

.count {
  display: flex;
  align-items: center;
  position: absolute;
  right: 20rpx;
  bottom: 30rpx;
  box-sizing: border-box;
  border: 2rpx solid #D8D8D8;
  border-radius: 2rpx;
  overflow: hidden;
}

.count .alter {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 66rpx;
  height: 60rpx;
}

.count input {
  display: block;
  width: 40rpx;
  padding: 0 24rpx;
  line-height: 60rpx;
  text-align: center;
  font-size: 26rpx;
  color: #222222;
  border-left: 2rpx solid #D8D8D8;
  border-right: 2rpx solid #D8D8D8;
}

.amount {
  position: fixed;
  bottom: 0;
  padding-left: 20rpx;
  width: 100%;
  line-height: 78rpx;
  font-size: 26rpx;
  background: white;
  z-index: 99;
}

.amount .total {
  color: #222222;
}

.amount .money {
  font-weight: bold;
  color: #F32939;
}

.amount .pay {
  display: block;
  float: right;
  width: 200rpx;
  height: 100%;
  background: #F32939;
  text-align: center;
  font-size: 30rpx;
  color: white;
}

.amount .pay-count {
  display: block;
  float: right;
  padding: 0 20rpx;
  color: #666666;
}